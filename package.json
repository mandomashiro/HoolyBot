{
  "name": "projet",
  "version": "0.1.0",
  "description": "Projet discord Hooligan2k",
  "main": "main.js",
  "repository": " https://github.com/mandomashiro/HoolyBot.git",
  "author": "mandomashiro <mandojeebot@gmail.com",
  "license": "MIT",
  "dependencies": {
    "discord.js": "discordjs/discord.js",
    "mongoose": "^5.6.4",
    "node-fetch": "^3.1.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-aqua": "^7.2.1"
  },
  "eslintConfig": {
    "extends": "aqua/node",
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "warn",
        "double"
      ],
      "indent": [
        "error",
        2
      ],
      "curly": "off",
      "no-trailing-spaces": "off",
      "prefer-template": "off",
      "no-eval": "off"
    }
  }
}
